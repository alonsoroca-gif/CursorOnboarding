{
  "name": "cursor-setup",
  "displayName": "Cursor Setup",
  "description": "Start the Cursor onboarding chat (Part 2: sign-in, smoke test, mental model). Run after you've installed and opened Cursor.",
  "version": "0.1.0",
  "publisher": "cursor-onboarding-kit",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cursorSetup.startOnboardingChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursorSetup.startOnboardingChat",
        "title": "Cursor Setup: Start onboarding chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  }
}
